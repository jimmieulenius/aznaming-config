{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[^$]": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": ["childDictionary", "dictionary", "freeText", "instance", "unique"]
        }
      },
      "required": ["type"],
      "oneOf": [
        {
          "properties": {
            "type": { "const": "dictionary" }
          },
          "required": ["type"],
          "then": {
            "properties": {
              "source": {
                "type": "object",
                "patternProperties": {
                  ".": { "type": "string" }
                },
                "additionalProperties": false
              }
            },
            "required": ["source"],
            "additionalProperties": false
          }
        },
        {
          "properties": {
            "type": { "const": "childDictionary" }
          },
          "required": ["type"],
          "then": {
            "properties": {
            },
            "required": [],
            "additionalProperties": false
          }
        }
      ]
    }
  },
  "additionalProperties": false
}
